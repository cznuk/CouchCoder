# CouchCoder runtime configuration. Update these values with your local secrets before building.
# SSH connection details
SSH_HOST=your-mac.local
SSH_PORT=22
SSH_USERNAME=your-username
SSH_PRIVATE_KEY_PATH=/Users/you/.ssh/couchcoder_key
# Alternatively, inline the key by setting SSH_PRIVATE_KEY with \n escapes inside quotes.
SSH_PRIVATE_KEY=""
SSH_PRIVATE_KEY_PASSPHRASE=

# Project discovery & defaults
PROJECTS_BASE_PATH=/Users/you/Projects
DEFAULT_AGENT=codex
DEVICE_UDID=YOUR-DEVICE-UDID
DEVELOPMENT_TEAM=XXXXXXXXXX
GIT_ONE_LINER="!git add . && git commit -m \"couch vibes\" && git push"
KEYCHAIN_PASSWORD=your-login-keychain-password
HIDDEN_PROJECTS_KEY=com.example.couchcoder.hiddenProjects
PINNED_PROJECTS_KEY=com.example.couchcoder.pinnedProjects
PINNED_PROJECTS_MAX_COUNT=9
PROJECT_ACCENT_COLORS_KEY=com.example.couchcoder.projectAccentColors
NEW_PROJECT_BUNDLE_PREFIX=com.example
NEW_PROJECT_DEPLOYMENT_TARGET=17.0
